{
  "presets": [
    [
      "@babel/preset-env",
      {
        // "debug": true,
        "corejs": {
          "version": "3.27.2",
          "proposals": true
        },
        "loose": true,
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", {"loose": false}],
    ["@babel/plugin-proposal-private-property-in-object", {"loose": false}],
    ["@babel/plugin-proposal-private-methods",{"loose": false}],
    "@babel/plugin-transform-runtime"
  ]
}