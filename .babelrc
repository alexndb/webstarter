{
  "presets": [
    [
      "@babel/preset-env",
      {
        // "debug": true,
        "corejs": {
          "version": "3.12.1",
          "proposals": true
        },
        "loose": true,
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": false
      }
    ],
    [
      "@babel/plugin-proposal-private-methods",
      {
        "loose": false
      }
    ],
    "@babel/plugin-transform-runtime"
  ]
}