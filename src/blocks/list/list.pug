mixin list(mods)
  -
    var allMods = '';
    if (typeof(mods) !== 'undefined' && mods) {
      var modsList = mods.split(',');
      for (var i = 0; i < modsList.length; i++) {
        allMods = allMods + ' _' + modsList[i].trim();
      }
    }

  +b.UL(href=href class=`list${allMods}`)&attributes(attributes)
    block