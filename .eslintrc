{
  "extends": "airbnb-base",
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "rules": {
    "linebreak-style": 0,
    "no-restricted-syntax": 0,
    "no-new": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-cycle": 0,
    "arrow-parens": 0,
    "no-unused-vars": 0,
    "comma-dangle": [
      "error",
      "never"
    ],
    "eol-last": [
      "error",
      "never"
    ],
    "semi": [
      "error",
      "never"
    ],
    "object-curly-spacing": [
      "error",
      "never"
    ],
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ]
  }
}